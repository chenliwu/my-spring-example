<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chenlw.java.web.utils.mapper.UserMapper">
    <resultMap id="BaseResultMap" type="com.chenlw.spring.example.entity.SysLog">
        <id column="id" jdbcType="VARCHAR" property="id"/>
        <result column="login_name" jdbcType="VARCHAR" property="loginName"/>
        <result column="password" jdbcType="VARCHAR" property="password"/>
    </resultMap>
    <sql id="Base_Column_List">

    </sql>

    <select id="selectUserLogList" resultType="com.chenlw.spring.example.entity.SysLog">
        SELECT
            SLOG.*
        FROM sys_log SLOG
        INNER JOIN sys_user SUER ON SUER.ID = SLOG.USER_ID
    </select>

</mapper>